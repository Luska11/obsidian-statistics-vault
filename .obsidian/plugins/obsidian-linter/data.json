    {
      "label": "Canvas",
      "match": "\\.canvas$",
      "flags": "i"
    },
    {
      "label": "Excalidraw",
      "match": "\\.excalidraw.md$",
      "flags": "i"
    },
    {
      "label": "Custom Prompts",
      "match": "\\Copilot Custom Prompts",
      "flags": "i"
    }
  ],
  "linterLocale": "system-default",
  "logLevel": "ERROR",
  "lintCommands": [
    {
      "id": "digitalgarden:dg-mark-note-for-publish",
      "name": "Digital Garden: Add publish flag",
      "enabled": true
    }
  ],
  "customRegexes": [
    {
      "label": "Replace invalid inline LaTeX",
      "find": "\\\\\\(\\s*(.*?)\\s*\\\\\\)",
      "replace": "$$$1$$",
      "flags": "g",
      "enabled": true
    },
    {
      "label": "Replace invalid multiline LaTeX",
      "find": "(?:\\\\t)?\\\\\\[([\\s\\S]*?)(?:\\\\t)?\\\\\\]",
      "replace": "$$$\\n$1\\n$$$",
      "flags": "g",
      "enabled": true
    },
    {
      "label": "Add line breaks to multiline LaTeX",
      "find": "\\$\\$(?!\\n)([\\s\\S]+?)(?<!\\n)\\$\\$",
      "replace": "$$$\\n$1\\n$$$",
      "flags": "g",
      "enabled": true
    },
    {
      "label": "Add line in between code blocks",
      "find": "(^\\s*\\$\\$\\s*\\n[\\s\\S]*?\\n\\s*\\$\\$\\s*)(?=\\s*\\n?\\s*\\$\\$)",
      "replace": "$1\\n",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Remove extra lines from multiline LaTex",
      "find": "\\$\\$\\n([\\s\\S]*?)\\n\\$\\$",
      "replace": "$$$\\n$1\\n$$$",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Remove bold from links",
      "find": "\\*\\*\\[\\[([^\\]]+?)\\]\\]\\*\\*",
      "replace": "[[$1]]",
      "flags": "g",
      "enabled": true
    },
    {
      "label": "Remove bold from normal links",
      "find": "\\*\\*\\[([^\\]]+?)\\]\\(([^)]+?)\\)\\*\\*",
      "replace": "[$1]($2)",
      "flags": "g",
      "enabled": true
    }
  ],
  "commonStyles": {
    "aliasArrayStyle": "single-line",
    "tagArrayStyle": "single-line",
    "minimumNumberOfDollarSignsToBeAMathBlock": 2,
    "escapeCharacter": "\"",
    "removeUnnecessaryEscapeCharsForMultiLineArrays": false
  }
}